        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   // Get values from Bill Amount and Paid Amount
   var x = g_form.getValue('u_bill_amount');
   var y = g_form.getValue('u_paid_amount');

   // Calculate balance due
   if(x!=" ")
   {
   var res = x - y;

   // Set balance due on form
   g_form.setValue('u_balance_due', res);
   }
}

]]></script>
